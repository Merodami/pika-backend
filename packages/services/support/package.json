{
  "name": "@pika/support",
  "version": "0.0.0",
  "private": true,
  "description": "Support Service for Pika Platform",
  "author": "Pika",
  "type": "module",
  "main": "dist/index.js",
  "dependencies": {
    "@prisma/client": "^6.11.1",
